{
  "schemaVersion": 1,
  "id": "sc",
  "version": "${version}",
  "name": "Summer Cottage",
  "description": "More summer fun :D",
  "authors": [
    "GalievDev"
  ],
  "contact": {
    "homepage": "https://galievdev.github.io/"
  },
  "license": "MIT Licence",
  "icon": "assets/sc/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.galiev.sc.SummerCottage"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "dev.galiev.sc.client.SummerCottageClient"
      }
    ]
  },
  "mixins": [
    "sc.mixins.json"
  ],
  "custom": {
    "fstats": 2,
    "duckyupdater": {
      "modrinthId": "eJ2H87hd",
      "type": "release",
      "featured": false
    }
  },
  "depends": {
    "fabricloader": ">=${loaderVersion}",
    "fabric": "*",
    "fabric-language-kotlin": ">=${fabricKotlinVersion}",
    "minecraft": ">=${minecraftVersion}",
    "java": ">=${javaVersion}",
    "geckolib": ">=${geckolib}"
  },
  "suggests": {
    "fstats-api": ">=${fstats}",
    "ducky-updater": ">=${duckyUpdater}"
  }
}
