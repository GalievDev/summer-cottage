{
  "schemaVersion": 1,
  "id": "sc",
  "version": "${version}",
  "name": "Summer Cottage",
  "description": "More summer fun :D",
  "authors": [
    "GalievDev"
  ],
  "contact": {
    "homepage": "https://galievdev.github.io/"
  },
  "license": "MIT Licence",
  "icon": "assets/template/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.galiev.sc.SummerCottage"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "dev.galiev.sc.client.SummerCottageClient"
      }
    ],
    "fabric-datagen": [
      {
        "adapter": "kotlin",
        "value": "dev.galiev.sc.SummerCottageDataGenerator"
      }
    ]
  },
  "mixins": [
    "sc.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loaderVersion}",
    "fabric": "*",
    "fabric-language-kotlin": ">=${fabricKotlinVersion}",
    "minecraft": "${minecraftVersion}",
    "java": ">=${javaVersion}"
  }
}
